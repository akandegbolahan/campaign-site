    "dropCapPaddingDesktop": {
      "type": "object"
    },
    "dropCapPaddingTablet": {
      "type": "object"
    },
    "dropCapPaddingMobile": {
      "type": "object"
    },
    "dropCapMarginDesktop": {
      "type": "object"
    },
    "dropCapMarginTablet": {
      "type": "object"
    },
    "dropCapMarginMobile": {
      "type": "object"
    },
