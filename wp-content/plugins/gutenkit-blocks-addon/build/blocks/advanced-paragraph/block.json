    "focusedTextTypography": {
      "type": "object"
    },
    "focusedTextDecorationColor": {
      "type": "string"
    },
    "focusedTextShadow": {
      "type": "object"
    },
    "focusedTextPaddingDesktop": {
      "type": "object"
    },
    "focusedTextPaddingTablet": {
      "type": "object"
    },
